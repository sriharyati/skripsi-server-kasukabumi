<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">Data Testimoni</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>Nama</th>
            <th>Rate</th>
            <th>Family Name</th>
            <th>Family Occupation</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < testimonial.length; i++){ %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= testimonial[i].name %></td>
            <td><%= testimonial[i].rate %></td>
            <td><%= testimonial[i].familyName %></td>
            <td><%= testimonial[i].familyOccupation %></td>
            <td>
              <a href="/admin/testimonial/<%= testimonial[i].id %>" class="btn btn-success btn-circle btn-sm button-update"><i
                  class=" fas fa-eye"></i></a>
            </td>
          </tr>
          <% } %>
        </tbody>
        
      </table>
    </div>
  </div>
</div>